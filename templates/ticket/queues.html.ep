% content_for queues => begin

	% foreach my $section (@{$sections}){
		<h3><%= $section->[0] %></h3>
		<table class="table table-striped table-hover">
			<thead>
				<th>Ticket Number</th>
				<th>Contact</th>
				<th>Synopsis</th>
				<th>Technician</th>
				<th>Status</th>
				<th>Submitted Date</th>
			</thead>
			<tbody>
			% foreach my $ticket (@{$queues->{$section->[0]}}){
				<tr data-id="<%= $ticket->{'id'} %>">
					<td><%= $ticket->{'ticket'} %></td>
					<td><%= $ticket->{'contact'} %></td>
					<td><%= $ticket->{'synopsis'} %></td>
					<td><%= $ticket->{'technician'} %></td>
					<td><%= $ticket->{'status'} %></td>
					<td><%= $ticket->{'genesis'} %></td>
				</tr>
			% }
			</tbody>
		</table>
	%}

% end